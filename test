  1 #include <stdio.h>
  2 #include <stdlib.h>
  3 #include <malloc.h>
  4 #define FIX NULL
  5 typedef struct b_tree
  6 {
  7     char name;
  8         struct b_tree *left;
  9         struct b_tree *right;
 10 }b_tree;
 11 int main(){
 12     struct b_tree *p;
 13     b_tree a0={'/',NULL,NULL},a1={'1',NULL,NULL}, a2={'2',NULL,NULL}, a3={'3',NULL,NULL}, a4={'4',NULL,NULL}, a5={'5',NULL,NULL}, a6={'6',NULL,NULL}; // 파일 만들 때 동적할당주소를 left or right 값에 넣어서 할 것
 14     a0.left=&a1;
 15     a1.right=&a2;
 16     a2.right=&a3;
 17     a3.right=&a4; // 예시로 파일 존재할 경우 출력
 18 if(a0.left!=NULL)
 19     p=&a1;          //부모 디렉터리 왼쪽 값이 NULL이 아닐경우 하위 p값에 왼쪽 주소를 삽입 해줌
 20
 21             while(p!=NULL)
 22         {
 23                     printf("%c",p->name);
 24                     p=p->right;}
 25
 26     return 0;}
